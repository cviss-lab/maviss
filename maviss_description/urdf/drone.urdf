<?xml version="1.0"?>
<robot name="maviss_drone" xmlns:xacro="http://www.ros.org/wiki/xacro">
  <link name="base_link">
    <collision name="maviss_drone_collision">
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="file://$(find maviss_drone)/meshes/yunque-M.dae"/>
      </geometry>
    </collision>
    <visual name="maviss_drone_visual">
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="file://$(find maviss_drone)/meshes/yunque-M.dae"/>
      </geometry>
      <inertial>
          <origin xyz="0 0 0" rpy="0 0 0"/>
          <mass value="12" />
          <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0" />
      </inertial>
    </visual>
  </link>
</robot>
